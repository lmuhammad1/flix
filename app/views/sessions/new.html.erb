<header>
  <h1>Sign In</h1>
</header>

<%= form_tag(session_path) do %>
  <fieldset>
    <ol>
      <li>
      <%= label_tag :email_or_username %>
      <%= text_field_tag :email_or_username, nil %>
      </li>
      <li>
      <%= label_tag :password %>
      <%= password_field_tag :password, nil %>
      </li>
    </ol>
    <p>
    <%= submit_tag 'Sign In' %>
    </p>
  </fieldset>
<% end %>
